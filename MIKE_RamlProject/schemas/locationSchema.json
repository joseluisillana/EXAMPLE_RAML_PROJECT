{
  "$schema": "http://json-schema.org/draft-03/schema",
  "required": true,
  "type": "object",
  "properties": {
    "geolocation": {
      "required": true,
      "type": "object",
      "properties": {
        "lat": {
          "required": true,
          "type": "string"
        },
        "lon": {
          "required": true,
          "type": "string"
        }
      }
    },
    "branch": {
      "required": true,
      "type": "object",
      "properties": {
        "branch_id": {
          "required": true,
          "type": "string"
        },
        "branch_code": {
          "required": true,
          "type": "string"
        },
        "operative_branch": {
          "required": true,
          "type": "string"
        },
        "bank": {
          "required": true,
          "type": "string"
        },
        "operative_bank": {
          "required": true,
          "type": "string"
        },
        "branch_address": {
          "required": true,
          "type": "object",
          "properties": {
            "province": {
              "required": true,
              "type": "number"
            },
            "post_code": {
              "required": true,
              "type": "number"
            },
            "country": {
              "required": true,
              "type": "string"
            }
          }
        },
        "region": {
          "required": true,
          "type": "string"
        },
        "head_branch": {
          "required": true,
          "type": "string"
        }
      }
    }
  }
}
